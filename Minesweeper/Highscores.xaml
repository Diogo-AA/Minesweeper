<Window x:Class="Minesweeper.Highscores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minesweeper"
        mc:Ignorable="d"
        Icon="Resources\icon.ico"
        Title="Highscore" Height="200" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Name="labelActualScore" Content="Your time was: XX:XX:XX" HorizontalAlignment="Center" Margin="0,48,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelBestScore" Content="Your best time is: XX:XX:XX" HorizontalAlignment="Center" Margin="0,92,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
